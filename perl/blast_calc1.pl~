#!/usr/bin/perl

use strict;
use lib 'modules/IPIG/';

BEGIN {
    require "BlastParser.class";
    import BlastParser;
}



package Main;

use strict;

use warnings;

use base 'Exporter';

our @EXPORT = qw(main);

sub new {
    my $class = shift;
    return bless {}, $class;
}

sub main {
    my $parser = new BlastParser();
    $parser->parse(shift(@ARGV));
}

Main::main();
